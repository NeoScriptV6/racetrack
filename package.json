{
    "name": "racetrack",
    "version": "1.0.0",
    "description": "npm install http-server --save-dev\nnpm install concurrently --save-dev",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run frontend\" \"npm run backend\" \"npm run backend-frontend-tunnel\"",
        "backend-frontend-tunnel": "wait-on tcp:3000 && node start-backend-frontend-tunnel.js",
        "dev": "concurrently \"npm run frontend\" \"cross-env NODE_ENV=development npm run backend\" \"npm run backend-frontend-tunnel\"",
        "frontend": "cd frontend && npm run dev",
        "backend": "nodemon ./backend/index.js",
        "local:start": "concurrently \"npm run frontend\" \"npm run backend\" \"npm run set-backend-url\"",
        "local:dev" : "concurrently \"npm run frontend\" \"cross-env NODE_ENV=development npm run backend\" \"npm run set-backend-url\"",
        "set-backend-url": "node set-backend-url.js"
    },
